alias ggg='git log --abbrev-commit --all --graph --decorate --color'
alias gg='ggg --oneline'
alias gs='git status'
alias gd='git diff'
alias gh='git diff --cached'
alias gu='git add -u'
alias gr='git reset HEAD'
alias g.='git checkout --'
alias gc='git commit -m'
alias gcu='git commit -au -m'
alias gl='git log -1'
alias g^='git diff HEAD{^,}'
alias gan='git commit --amend'
alias gup='git add -up'
alias gcp='git checkout -p --'
alias gf='git fetch origin'
alias gpush='git push origin'
alias gpull='git pull origin'
